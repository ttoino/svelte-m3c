{
    "name": "svelte-m3c",
    "version": "0.0.5",
    "license": "GPL-3.0-only",
    "scripts": {
        "dev": "vite dev",
        "build": "trap 'kill $!' EXIT; pnpm run dev --host --strict-port & vite build && pnpm run prepack",
        "preview": "vite preview",
        "prepare": "svelte-kit sync || echo ''",
        "prepack": "svelte-kit sync && svelte-package && publint",
        "check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
        "check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
        "format": "prettier --check .",
        "format:fix": "prettier --write .",
        "lint": "eslint .",
        "lint:fix": "eslint --fix .",
        "test": "vitest"
    },
    "files": [
        "dist",
        "!dist/**/*.test.*",
        "!dist/**/*.spec.*"
    ],
    "sideEffects": [
        "**/*.css"
    ],
    "svelte": "./dist/index.js",
    "types": "./dist/index.d.ts",
    "style": "./dist/style/index.css",
    "type": "module",
    "exports": {
        ".": {
            "types": "./dist/index.d.ts",
            "style": "./dist/style/index.css",
            "svelte": "./dist/index.js",
            "default": "./dist/index.js"
        },
        "./paints/*": {
            "types": "./dist/paints/*.d.ts",
            "svelte": "./dist/paints/*.js",
            "default": "./dist/paints/*.js"
        },
        "./palette": {
            "types": "./dist/palette.d.ts",
            "svelte": "./dist/palette.js",
            "default": "./dist/palette.js"
        },
        "./style": {
            "types": "./dist/style.d.ts",
            "svelte": "./dist/style.js",
            "default": "./dist/style.js"
        },
        "./variants": {
            "types": "./dist/variants.d.ts",
            "svelte": "./dist/variants.js",
            "default": "./dist/variants.js"
        },
        "./*": {
            "style": "./dist/style/*.css"
        }
    },
    "peerDependencies": {
        "svelte": "^5.33.2"
    },
    "dependencies": {
        "@fontsource-variable/roboto": "^5.2.9",
        "@fontsource-variable/roboto-mono": "^5.2.8",
        "@material/material-color-utilities": "^0.3.0",
        "bits-ui": "^2.14.4",
        "css-paint-polyfill": "^3.4.0",
        "material-symbols": "^0.40.2",
        "tailwind-merge": "^3.4.0",
        "tailwind-variants": "^3.2.2",
        "tailwindcss": "^4.1.18"
    },
    "devDependencies": {
        "@eslint/compat": "^2.0.0",
        "@eslint/js": "^9.39.2",
        "@sveltejs/adapter-static": "^3.0.10",
        "@sveltejs/kit": "^2.49.2",
        "@sveltejs/package": "^2.5.7",
        "@sveltejs/vite-plugin-svelte": "^6.2.1",
        "@tailwindcss/vite": "^4.1.18",
        "@types/node": "^25.0.3",
        "@vitest/browser-playwright": "^4.0.16",
        "dedent": "^1.7.1",
        "eslint": "^9.39.2",
        "eslint-config-prettier": "^10.1.8",
        "eslint-plugin-perfectionist": "^5.2.0",
        "eslint-plugin-svelte": "^3.13.1",
        "globals": "^16.5.0",
        "new-javascript": "^0.4.7",
        "playwright": "1.57.0",
        "prettier": "^3.7.4",
        "prettier-plugin-svelte": "^3.4.1",
        "prettier-plugin-tailwindcss": "^0.7.2",
        "publint": "^0.3.16",
        "shiki": "^3.20.0",
        "svelte": "^5.46.1",
        "svelte-check": "^4.3.5",
        "typescript": "^5.9.3",
        "typescript-eslint": "^8.51.0",
        "vite": "^7.3.0",
        "vitest": "^4.0.16",
        "vitest-browser-svelte": "^2.0.1"
    },
    "keywords": [
        "svelte"
    ],
    "pnpm": {
        "onlyBuiltDependencies": [
            "esbuild"
        ]
    },
    "packageManager": "pnpm@10.14.0+sha512.ad27a79641b49c3e481a16a805baa71817a04bbe06a38d17e60e2eaee83f6a146c6a688125f5792e48dd5ba30e7da52a5cda4c3992b9ccf333f9ce223af84748"
}
