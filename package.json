{
    "name": "svelte-m3c",
    "version": "0.0.5",
    "license": "GPL-3.0-only",
    "scripts": {
        "dev": "vite dev",
        "build": "vite build && npm run prepack",
        "preview": "vite preview",
        "prepare": "svelte-kit sync || echo ''",
        "prepack": "svelte-kit sync && svelte-package && publint",
        "check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
        "check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
        "format": "prettier --check .",
        "format:fix": "prettier --write .",
        "lint": "eslint .",
        "lint:fix": "eslint --fix ."
    },
    "files": [
        "dist",
        "!dist/**/*.test.*",
        "!dist/**/*.spec.*"
    ],
    "sideEffects": [
        "**/*.css"
    ],
    "svelte": "./dist/index.js",
    "types": "./dist/index.d.ts",
    "style": "./dist/style/index.css",
    "type": "module",
    "exports": {
        ".": {
            "types": "./dist/index.d.ts",
            "style": "./dist/style/index.css",
            "svelte": "./dist/index.js",
            "default": "./dist/index.js"
        },
        "./paints/*": {
            "types": "./dist/paints/*.d.ts",
            "svelte": "./dist/paints/*.js",
            "default": "./dist/paints/*.js"
        },
        "./palette": {
            "types": "./dist/palette.d.ts",
            "svelte": "./dist/palette.js",
            "default": "./dist/palette.js"
        },
        "./style": {
            "types": "./dist/style.d.ts",
            "svelte": "./dist/style.js",
            "default": "./dist/style.js"
        },
        "./variants": {
            "types": "./dist/variants.d.ts",
            "svelte": "./dist/variants.js",
            "default": "./dist/variants.js"
        },
        "./*": {
            "style": "./dist/style/*.css"
        }
    },
    "peerDependencies": {
        "svelte": "^5.33.2"
    },
    "dependencies": {
        "@material/material-color-utilities": "^0.3.0",
        "bits-ui": "^2.11.1",
        "css-paint-polyfill": "^3.4.0",
        "material-symbols": "^0.36.0",
        "tailwind-merge": "^3.3.1",
        "tailwind-variants": "^3.1.1",
        "tailwindcss": "^4.1.13"
    },
    "devDependencies": {
        "@eslint/compat": "^1.4.0",
        "@eslint/js": "^9.36.0",
        "@sveltejs/adapter-static": "^3.0.9",
        "@sveltejs/kit": "^2.43.2",
        "@sveltejs/package": "^2.5.4",
        "@sveltejs/vite-plugin-svelte": "^6.2.0",
        "@tailwindcss/vite": "^4.1.13",
        "eslint": "^9.36.0",
        "eslint-config-prettier": "^10.1.8",
        "eslint-plugin-perfectionist": "^4.15.0",
        "eslint-plugin-svelte": "^3.12.4",
        "globals": "^16.4.0",
        "new-javascript": "^0.4.7",
        "prettier": "^3.6.2",
        "prettier-plugin-svelte": "^3.4.0",
        "prettier-plugin-tailwindcss": "^0.6.14",
        "publint": "^0.3.13",
        "svelte": "^5.39.5",
        "svelte-check": "^4.3.2",
        "typescript": "^5.9.2",
        "typescript-eslint": "^8.44.1",
        "vite": "^7.1.7"
    },
    "keywords": [
        "svelte"
    ],
    "pnpm": {
        "onlyBuiltDependencies": [
            "esbuild"
        ]
    },
    "packageManager": "pnpm@10.14.0+sha512.ad27a79641b49c3e481a16a805baa71817a04bbe06a38d17e60e2eaee83f6a146c6a688125f5792e48dd5ba30e7da52a5cda4c3992b9ccf333f9ce223af84748"
}
