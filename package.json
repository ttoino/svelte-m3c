{
    "name": "svelte-m3c",
    "version": "0.0.1",
    "license": "GPL-3.0-only",
    "scripts": {
        "dev": "vite dev",
        "build": "vite build && npm run prepack",
        "preview": "vite preview",
        "prepare": "svelte-kit sync || echo ''",
        "prepack": "svelte-kit sync && svelte-package && publint",
        "check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
        "check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
        "format": "prettier --check .",
        "format:fix": "prettier --write .",
        "lint": "eslint .",
        "lint:fix": "eslint --fix ."
    },
    "files": [
        "dist",
        "!dist/**/*.test.*",
        "!dist/**/*.spec.*"
    ],
    "sideEffects": [
        "**/*.css"
    ],
    "svelte": "./dist/index.js",
    "types": "./dist/index.d.ts",
    "style": "./dist/style/index.css",
    "type": "module",
    "exports": {
        ".": {
            "types": "./dist/index.d.ts",
            "style": "./dist/style/index.css",
            "svelte": "./dist/index.js"
        },
        "./*": {
            "types": "./dist/**/*.d.ts",
            "style": "./dist/style/*.css",
            "svelte": "./dist/**/*.svelte"
        }
    },
    "peerDependencies": {
        "svelte": "^5.0.0"
    },
    "devDependencies": {
        "@eslint/compat": "^1.2.5",
        "@eslint/js": "^9.18.0",
        "@material/material-color-utilities": "^0.3.0",
        "@sveltejs/adapter-auto": "^4.0.0",
        "@sveltejs/adapter-static": "^3.0.8",
        "@sveltejs/kit": "^2.16.0",
        "@sveltejs/package": "^2.0.0",
        "@sveltejs/vite-plugin-svelte": "^5.0.0",
        "@tailwindcss/vite": "^4.0.0",
        "bits-ui": "^1.3.13",
        "clsx": "^2.1.1",
        "eslint": "^9.18.0",
        "eslint-config-prettier": "^10.0.1",
        "eslint-plugin-perfectionist": "^4.12.1",
        "eslint-plugin-svelte": "^3.0.0",
        "globals": "^16.0.0",
        "material-symbols": "^0.29.2",
        "mdsvex": "^0.12.5",
        "prettier": "^3.4.2",
        "prettier-plugin-svelte": "^3.3.3",
        "prettier-plugin-tailwindcss": "^0.6.11",
        "publint": "^0.3.2",
        "svelte": "^5.0.0",
        "svelte-check": "^4.0.0",
        "tailwind-merge": "^3.0.2",
        "tailwind-variants": "^1.0.0",
        "tailwindcss": "^4.0.0",
        "typescript": "^5.0.0",
        "typescript-eslint": "^8.32.0",
        "vite": "^6.0.0"
    },
    "keywords": [
        "svelte"
    ],
    "pnpm": {
        "onlyBuiltDependencies": [
            "esbuild"
        ]
    },
    "packageManager": "pnpm@10.5.0+sha512.11106a5916c7406fe4b8cb8e3067974b8728f47308a4f5ac5e850304afa6f57e2847d7950dfe78877d8d36bfb401d381c4215db3a4c3547ffa63c14333a6fa51"
}
