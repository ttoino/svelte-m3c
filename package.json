{
    "name": "svelte-m3c",
    "version": "0.0.2",
    "license": "GPL-3.0-only",
    "scripts": {
        "dev": "vite dev",
        "build": "vite build && npm run prepack",
        "preview": "vite preview",
        "prepare": "svelte-kit sync || echo ''",
        "prepack": "svelte-kit sync && svelte-package && publint",
        "check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
        "check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
        "format": "prettier --check .",
        "format:fix": "prettier --write .",
        "lint": "eslint .",
        "lint:fix": "eslint --fix ."
    },
    "files": [
        "dist",
        "!dist/**/*.test.*",
        "!dist/**/*.spec.*"
    ],
    "sideEffects": [
        "**/*.css"
    ],
    "svelte": "./dist/index.js",
    "types": "./dist/index.d.ts",
    "style": "./dist/style/index.css",
    "type": "module",
    "exports": {
        ".": {
            "types": "./dist/index.d.ts",
            "style": "./dist/style/index.css",
            "svelte": "./dist/index.js",
            "default": "./dist/index.js"
        },
        "./palette": {
            "types": "./dist/palette.d.ts",
            "svelte": "./dist/palette.js",
            "default": "./dist/palette.js"
        },
        "./style": {
            "types": "./dist/style.d.ts",
            "svelte": "./dist/style.js",
            "default": "./dist/style.js"
        },
        "./variants": {
            "types": "./dist/variants.d.ts",
            "svelte": "./dist/variants.js",
            "default": "./dist/variants.js"
        },
        "./*": {
            "style": "./dist/style/*.css"
        }
    },
    "peerDependencies": {
        "svelte": "^5.33.2"
    },
    "devDependencies": {
        "@eslint/compat": "^1.2.9",
        "@eslint/js": "^9.28.0",
        "@material/material-color-utilities": "^0.3.0",
        "@sveltejs/adapter-static": "^3.0.8",
        "@sveltejs/kit": "^2.21.2",
        "@sveltejs/package": "^2.3.11",
        "@sveltejs/vite-plugin-svelte": "^5.1.0",
        "@tailwindcss/vite": "^4.1.8",
        "bits-ui": "^2.8.11",
        "clsx": "^2.1.1",
        "css-paint-polyfill": "^3.4.0",
        "eslint": "^9.28.0",
        "eslint-config-prettier": "^10.1.5",
        "eslint-plugin-perfectionist": "^4.14.0",
        "eslint-plugin-svelte": "^3.9.1",
        "globals": "^16.2.0",
        "material-symbols": "^0.31.5",
        "new-javascript": "^0.4.7",
        "prettier": "^3.5.3",
        "prettier-plugin-svelte": "^3.4.0",
        "prettier-plugin-tailwindcss": "^0.6.12",
        "publint": "^0.3.12",
        "svelte": "^5.33.14",
        "svelte-check": "^4.2.1",
        "tailwind-merge": "^3.3.0",
        "tailwind-variants": "^1.0.0",
        "tailwindcss": "^4.1.8",
        "typescript": "^5.8.3",
        "typescript-eslint": "^8.33.1",
        "vite": "^6.3.5"
    },
    "keywords": [
        "svelte"
    ],
    "pnpm": {
        "onlyBuiltDependencies": [
            "esbuild"
        ]
    },
    "packageManager": "pnpm@10.11.1"
}
